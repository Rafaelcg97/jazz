        <Viewbox  Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="16" Grid.RowSpan="2" HorizontalAlignment="Left">
            <StackPanel Orientation="Horizontal">
                <Button x:Name="salir_" Background="Transparent" VerticalAlignment="Top" BorderBrush="Transparent" Margin="10,10,0,0" Click="salir__Click" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">
                    <Image Source="/imagenes/atras.png" Width="60" Height="65"/>
                </Button>
                <Label Foreground="Green" Margin="570,0,0,0" FontWeight="Bold" VerticalAlignment="Center" FontSize="60">Solicitud de Materiales</Label>
            </StackPanel>
        </Viewbox>
        <Viewbox Grid.Row="3" Grid.ColumnSpan="4">
            <StackPanel Orientation="Horizontal">
                <Label FontWeight="Bold" Padding="0" Content="Usuario:" Margin="0,0,10,0"/>
                <PasswordBox x:Name="passwordboxUsuario" Height="20" Width="150" PasswordChanged="passwordboxUsuario_PasswordChanged"/>
                <Label x:Name="labelUsuario" Content="*" Width="50" VerticalContentAlignment="Center" />
            </StackPanel>
        </Viewbox>
        <Viewbox Grid.Row="4" Grid.ColumnSpan="12" Grid.RowSpan="9" VerticalAlignment="Top" HorizontalAlignment="Left">
            <StackPanel Orientation="Horizontal" Margin="30,10,0,0">
                <StackPanel>
                    <Label x:Name="labelUbicacion" Content="0" Visibility="Collapsed"/>
                    <Label FontFamily="calibri" FontWeight="Bold" FontSize="15" Content="Módulo" VerticalContentAlignment="Bottom"/>
                    <ListBox x:Name="listBoxModulo" Height="220" Width="150" FontFamily="calibri" FontSize="15" FontWeight="Bold" SelectionChanged="listBoxModulo_SelectionChanged"/>
                </StackPanel>
                <StackPanel Margin="10,0,0,0">
                    <Label FontFamily="calibri" FontWeight="Bold" FontSize="15" Content="Lote" VerticalContentAlignment="Bottom"/>
                    <TextBox x:Name="textBoxBuscarLote" Height="20" Width="150" VerticalContentAlignment="Center" TextChanged="textBoxBuscarLote_TextChanged"/>
                    <ListBox x:Name="listBoxLote" FontFamily="calibri" Height="200" Width="150" FontSize="15" FontWeight="Bold" SelectionChanged="listBoxLote_SelectionChanged">
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <Label Content="{Binding NumeroLote}"/>
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                </StackPanel>
                <StackPanel Margin="15,45,0,0">
                    <StackPanel Orientation="Horizontal" Margin="0,0,0,0" Background="AliceBlue" Width="150" Height="100">
                        <Label Content="Lote SMED" FontWeight="Bold" FontSize="17" Margin="0,0,10,0" VerticalAlignment="Center"/>
                        <CheckBox x:Name="chBoxLoteSmed" VerticalAlignment="Center" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="0,5,0,0" Background="AliceBlue" Width="150" Height="100">
                        <Label Content="Caja Parcial" FontWeight="Bold" FontSize="17" Margin="0,0,10,0" VerticalAlignment="Center"/>
                        <CheckBox x:Name="chBoxCajaParcial" VerticalAlignment="Center" Checked="chBoxCajaParcial_Checked" Unchecked="chBoxCajaParcial_Unchecked" />
                    </StackPanel>
                </StackPanel>
                <StackPanel Orientation="Vertical" Margin="10,35,0,0">
                    <StackPanel Orientation="Horizontal" Margin="10">
                        <Label FontFamily="calibri" FontWeight="Bold" Content="Estilo:" FontSize="18"/>
                        <Label x:Name="labelEstilo" FontFamily="calibri" FontSize="18" FontWeight="Bold" Content="----"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="10">
                        <Label FontFamily="calibri" FontWeight="Bold" Content="Color:" FontSize="18"/>
                        <Label x:Name="labelColor" FontFamily="calibri" FontSize="18" FontWeight="Bold" Content="----"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="10">
                        <Label FontFamily="calibri" FontWeight="Bold" Content="Temporada:" FontSize="18"/>
                        <Label x:Name="labelTemporada" FontFamily="calibri" FontSize="18" FontWeight="Bold" Width="300" Content="----"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="10">
                        <Label FontFamily="calibri" FontWeight="Bold" Content="Piezas:" FontSize="18"/>
                        <Label x:Name="labelPiezas" FontFamily="calibri" FontSize="18" FontWeight="Bold" Content="----"/>
                    </StackPanel>
                </StackPanel>
            </StackPanel>
        </Viewbox>
        <Border BorderBrush="Black" BorderThickness="4" Grid.Column="10" Grid.Row="5" Grid.RowSpan="20" Grid.ColumnSpan="5" CornerRadius="10">
            <ListView x:Name="listViewListaMateriales" FontSize="15" FontWeight="Bold" Margin="5" KeyDown="listViewListaMateriales_KeyDown">
                <ListView.View>
                    <GridView>
                        <GridViewColumn Header="Lote" Width="150">
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <Label Content="{Binding NumeroLote}" FontSize="12" Foreground="White" Background="DarkRed" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Width="200"/>
                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>
                        <GridViewColumn Header="Material" Width="185" >
                            <GridViewColumn.CellTemplate>
