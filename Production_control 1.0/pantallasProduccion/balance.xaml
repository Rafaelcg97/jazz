<Page x:Class="Production_control_1._0.balance"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Production_control_1._0"
      xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
     xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      Title="balance" Background="white">
    <Grid>
        <!--#region  region control general del programa-->
        <StackPanel VerticalAlignment="Top">
            <Grid Background="#FF0069C0" Height="10" MouseLeftButtonDown="titleBar_MouseLeftButtonDown"/>
            <Grid Margin="5">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="10, 0" >
                    <Button Width="30" Height="30" Background="{x:Null}" BorderBrush="{x:Null}" Padding="0" Foreground="Gray" Click="ButtonMinimizar">
                        <materialDesign:PackIcon Kind="WindowMinimize"/>
                    </Button>
                    <Button Width="30" Height="30" Background="{x:Null}" BorderBrush="{x:Null}" Padding="0" Foreground="Gray" Margin="0 0 10 0" Click="ButtonMaximizar">
                        <materialDesign:PackIcon Kind="WindowMaximize"/>
                    </Button>
                    <Button Width="30" Height="30" Background="{x:Null}" BorderBrush="{x:Null}" Padding="0" Foreground="Gray" Click="ButtonSalir">
                        <materialDesign:PackIcon Kind="Close"/>
                    </Button>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="10, 0" >
                    <Button x:Name="ButtonOpen" HorizontalAlignment="Left" Height="30" Width="30" Background="{x:Null}" BorderBrush="{x:Null}" Padding="0" Foreground="Gray" Click="ButtonOpen_Click">
                        <materialDesign:PackIcon Kind="Menu" Foreground="#FF313131"/>
                    </Button>
                </StackPanel>
            </Grid>
        </StackPanel>
        <!--#endregion-->
        <!--#region menuLateral-->
        <Grid x:Name="GridMenu" Width="250" HorizontalAlignment="Left" Margin="-250 0 0 0" Background="Black" RenderTransformOrigin="0.5,0.5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Button x:Name="ButtonClose" Grid.Row="0" Grid.Column="3" HorizontalAlignment="Right" VerticalAlignment="Top" Foreground="White" Background="Transparent" BorderBrush="Transparent" Width="30" Height="30" Padding="0" Click="ButtonClose_Click" >
                <materialDesign:PackIcon Kind="Close"/>
            </Button>
            <Label Grid.Row="1" Grid.ColumnSpan="4" FontSize="20" Foreground="White" Content="Últimas Versiones" HorizontalAlignment="Center"/>
            <ItemsControl x:Name="UltimasVersiones" Grid.Row="2" Grid.RowSpan="2" Grid.ColumnSpan="4" Background="#FF22242C" >
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <Grid Margin="0,0,0,5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Label x:Name="modulo_orden" Content="{Binding Path=modulo}" Grid.ColumnSpan="4" FontWeight="Bold" Grid.Column="0"  Foreground="AliceBlue" />
                            <Label x:Name="modulo_prio" Content="{Binding Path=fechaModificacion}" Grid.ColumnSpan="3" Grid.Column="4" Foreground="AliceBlue" />
                            <Label x:Name="maquina_prio" Content="{Binding Path=version}" Grid.ColumnSpan="2" Grid.Column="7"  Foreground="AliceBlue" />
                        </Grid>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
            <StackPanel Grid.Row="4" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="4">
                <StackPanel Orientation="Horizontal">
                    <Label FontSize="15" Content="Estilo:" FontWeight="Bold" Foreground="White" Width="100"/>
                    <Label  x:Name="LabelEstiloGuardar"  FontSize="15"  Content="----" FontWeight="Bold" Foreground="White"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <Label FontSize="15" Content="Temporada:" FontWeight="Bold" Foreground="White" Width="100"/>
                    <Label  x:Name="LabelTemporadaGuardar"  FontSize="15"  Content="----" FontWeight="Bold" Foreground="White"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <Label FontSize="15" Content="Módulo:" FontWeight="Bold" Foreground="White" Width="100"/>
                    <Label  x:Name="LabelModuloGuardar"  FontSize="15"  Content="----" FontWeight="Bold" Foreground="White"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <Label FontSize="15" Content="Versión:" FontWeight="Bold" Foreground="White" Width="100"/>
                    <Label  x:Name="LabelVersionGuardar"  FontSize="15"  Content="----" FontWeight="Bold" Foreground="White"/>
                </StackPanel>
            </StackPanel>
            <Button x:Name="ButtonGuardarBalance" Margin="0,0,0,10" Grid.Row="6" Grid.Column="0" Background="Transparent" Grid.ColumnSpan="4" Grid.RowSpan="1" Click="ButtonGuardarBalance_Click" >
                <StackPanel Orientation="Horizontal">
                    <Image Source="/PantallasProduccion/imagenes/disquete.png" Height="40" Margin="0,0,35,0"/>
                    <Label FontSize="20" Foreground="White" Content="Guardar" VerticalAlignment="Center" Padding="0"/>
                </StackPanel>
            </Button>
            <Button x:Name="ButtonImprimirBalance" Margin="0,10,0,0" Grid.Row="7" Grid.Column="0" Background="Transparent" Grid.ColumnSpan="4" Grid.RowSpan="1" Click="ButtonImprimirBalance_Click">
                <StackPanel Orientation="Horizontal">
                    <Image Source="/PantallasProduccion/imagenes/impresora.png" Height="50" Margin="0,0,35,0"/>
                    <Label FontSize="20" Foreground="White" Content="Imprimir" VerticalAlignment="Center" Padding="0"/>
                </StackPanel>
            </Button>
            <Button Margin="0,20,0,20" Grid.Row="9" Grid.Column="0" Background="Transparent" Grid.ColumnSpan="4" Grid.RowSpan="1" x:Name="ButtonCerrarBalance" Click="ButtonCerrarBalance_Click" >
                <StackPanel Orientation="Horizontal">
                    <Image Source="/PantallasProduccion/imagenes/salir.png" Margin="0,0,30,0" />
                    <Label FontSize="17" Foreground="White" Content="Salir de Editor" VerticalAlignment="Center"/>
                </StackPanel>
            </Button>
        </Grid>
        <!--#endregion-->
        <!--#region areaDeTrabajo-->
        <Grid Margin="5,30,0,5" x:Name="GridBackground">
            <!--#region filas/columnas-->
            <Grid.RowDefinitions>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="22*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="23*"/>
                <RowDefinition Height="22*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <!--#endregion-->
            <Border BorderBrush="Black" BorderThickness="1" Margin="2" Padding="2" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="6" Grid.RowSpan="7">
                <StackPanel Orientation="Horizontal">
                    <StackPanel Margin="5">
                        <Label Content="Fecha"  Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="Estilo:"  Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="Temporada:"  Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="SAM:"  Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="Modulo:" Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="Versión:" Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2" Margin="0,2,0,0"/>
                    </StackPanel>
                    <StackPanel Margin="5">
                        <Label x:Name="fecha_" Content="----"   Grid.Column="3"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                        <Label x:Name="estilo_" Content="----"   Grid.Column="3"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                        <Label x:Name="temporada_" Content="----"  Grid.Column="3"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                        <Label x:Name="sam_" Content="----"   Grid.Column="3"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                        <StackPanel Orientation="Horizontal">
                            <ComboBox x:Name="modulo"  Grid.Row="5" Grid.Column="3" FontSize="14" FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2" Width="100" SelectionChanged="modulo_SelectionChanged"/>
                            <Button x:Name="agregarOperariosGnerale" Grid.Column="4" Grid.ColumnSpan="1" Click="agregarListaDeOperariosGenerale">
                                <Image Source="imagenes/desconocido.png" Grid.ColumnSpan="2" Margin="0,0,0,0" Grid.RowSpan="1" Height="15" Width="31" UseLayoutRounding="False" RenderTransformOrigin="0.758,0.5"/>
                            </Button>
                            <Button x:Name="agregar_operario" Grid.Column="4" Grid.ColumnSpan="1" Click="agregar_operario_Click">
                                <Image Source="/imagenes/nuevo_use.png" Grid.ColumnSpan="2" Margin="0,0,0,0" Grid.RowSpan="1" Height="15" Width="31" UseLayoutRounding="False" RenderTransformOrigin="0.758,0.5"/>
                            </Button>
                            <Popup x:Name="datos_operario" Width="420" Height="150">
                                <Border Background="#F9FAFA" CornerRadius="10">
                                    <StackPanel>
                                        <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                                            <Label FontWeight="Bold" Background="#F9FAFA" Width="150" Margin="0,5,0,25">Código de Operario:</Label>
                                            <TextBox x:Name="codigo_op" Width="200" Margin="10,5,0,25" TextChanged="codigo_op_TextChanged" ></TextBox>
                                            <Button x:Name="revision" Width="30" Height="30" Margin="0,5,0,25" Background="Transparent" BorderBrush="Transparent" IsEnabled="False" Visibility="Hidden">
                                                <Image Source="/imagenes/marca.png"></Image>
                                            </Button>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <Label FontWeight="Bold" Background="#F9FAFA" Width="150">Nombre de Operario:</Label>
                                            <TextBox x:Name="nombre_op" Width="200" Margin="10,0,0,0"></TextBox>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal" Margin="0,15,0,0" HorizontalAlignment="Center">
                                            <Button x:Name="agregar_op"  Width="30" Height="30" Margin="0,0,10,0" Background="Transparent" BorderBrush="Transparent" Click="agregar_op_Click">
                                                <Image Source="/imagenes/agregar-usuario.png"></Image>
                                            </Button>
                                            <Button x:Name="no_nomb" Width="30" Height="30" Margin="10,0,10,0" Background="Transparent" BorderBrush="Transparent" Click="no_nomb_Click">
                                                <Image Source="/imagenes/desconocido.png"></Image>
                                            </Button>
                                            <Button x:Name="agregar_cancel" Width="30" Height="30" Margin="10,0,0,0" Click="agregar_cancel_Click" Background="Transparent" BorderBrush="Transparent">
                                                <Image Source="/imagenes/boton-x.png"></Image>
                                            </Button>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                            </Popup>
                        </StackPanel>
                        <TextBox x:Name="version_" Grid.Column="3" FontFamily="calibri" FontWeight="Medium" Grid.ColumnSpan="2" Margin="0,2,0,0" KeyDown="solo_numeros" ></TextBox>
                    </StackPanel>
                </StackPanel>
            </Border>

            <Border BorderBrush="Black" BorderThickness="1" Margin="2" Padding="2" Grid.Column="6" Grid.Row="1" Grid.ColumnSpan="6" Grid.RowSpan="7">
                <StackPanel Orientation="Horizontal" Grid.ColumnSpan="3">
                    <StackPanel Margin="5">
                        <Label Content="Piezas de la corrida:" Grid.Row="1" Grid.Column="8" FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="Horas de Corrida:"  Grid.Row="2" Grid.Column="8"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="Operarios:" Grid.Row="4" Grid.Column="8"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="Eficiencia:" Grid.Row="3" Grid.Column="8" FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                        <Label Content="Piezas por Hora:" Grid.Row="5" Grid.Column="8" FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    </StackPanel>
                    <StackPanel Margin="5" Grid.ColumnSpan="3">
                        <TextBox x:Name="piezas_de_corrida"  Grid.Row="2" Grid.Column="11"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2" MinWidth="50" MaxWidth="150" TextChanged="maquinaRequerimiento" KeyDown="solo_numeros"/>
                        <TextBox x:Name="horas_de_corrida"  Grid.Row="1" Grid.Column="11"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"  MinWidth="50" MaxWidth="150" TextChanged="maquinaRequerimiento" KeyDown="solo_numeros"/>
                        <Label x:Name="operarios_" Content="0" Grid.Row="4" Grid.Column="11" FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2" MinWidth="50" MaxWidth="150" />
                        <Label x:Name="eficiencia_" Content="0"  Grid.Row="3" Grid.Column="11" FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"  MinWidth="50" MaxWidth="150"/>
                        <Label x:Name="piezas_por_hora" Content="0"  Grid.Row="5" Grid.Column="11"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                    </StackPanel>
                </StackPanel>
            </Border>

            <Border BorderBrush="Black" BorderThickness="1" Margin="2" Grid.Row="1" Grid.Column="12" Grid.RowSpan="7">
                <Slider x:Name="tamano"  Grid.Row="1" Grid.Column="12" Minimum="0" Maximum="2500" Orientation="Vertical" Grid.RowSpan="7" HorizontalAlignment="Center" ValueChanged="Slider_ValueChanged"/>
            </Border>
            <!--#region control_tab_de_datos-->
            <TabControl  Grid.Row="8" Grid.RowSpan="40" Grid.Column="0" Grid.ColumnSpan="13">
                <!--#region lista_de_operaciones-->
                <TabItem Header="Operaciones" ScrollViewer.HorizontalScrollBarVisibility="Visible">
                    <ListView x:Name="Operaciones" HorizontalAlignment="Stretch" FontSize="10" SelectionMode="Extended" PreviewMouseLeftButtonDown="obtenerDatosListView">
                        <ListView.View>
                            <GridView>
                                <GridViewColumn Header="Operacion" DisplayMemberBinding="{Binding Path=tituloOperacion}" Width="275" />
                                <GridViewColumn Header="Asignado">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid Grid.Column="4">
                                                <ProgressBar  Minimum="0" Maximum="1.01" Value="{Binding asignadoOperacion}" x:Name="progressBarAsignado" Loaded="progressBarAsignado_Loaded" Width="100"/>
                                                <TextBlock Text="{Binding asignadoOperacion, StringFormat=P1}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                            </Grid>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="SAM" DisplayMemberBinding="{Binding Path=samOperacion}" Width="40"/>
                                <GridViewColumn Header="Ajuste" DisplayMemberBinding="{Binding Path=ajusteMaquina}" Width="130"/>
                                <GridViewColumn Header="Requerido" DisplayMemberBinding="{Binding Path=requeridoOperacion}" Width="28"/>
                            </GridView>
                        </ListView.View>
                    </ListView>
                </TabItem>
                <!--#endregion-->
                <!--#region lista_de_operarios-->
                <TabItem Header="Operarios">
                    <ListView x:Name="Operarios" HorizontalAlignment="Stretch"  SelectionMode="Extended" PreviewMouseLeftButtonDown="obtenerDatosListView" KeyDown="Operarios_KeyDown" FontSize="8">
                        <ListView.View>
                            <GridView>
                                <GridViewColumn Header="Operarios" DisplayMemberBinding="{Binding Path=nombreOperario}" Width="275" />
                                <GridViewColumn Header="Asignado">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid Grid.Column="4">
                                                <ProgressBar  Minimum="0" Maximum="1.01" Value="{Binding asignadoOperario}" Loaded="progressBarAsignado_Loaded" Width="100"/>
                                                <TextBlock Text="{Binding asignadoOperario, StringFormat=P1}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Grid>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Plana" DisplayMemberBinding="{Binding Path=planaOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Rana" DisplayMemberBinding="{Binding Path=ranaOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Flat" DisplayMemberBinding="{Binding Path=flatOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Cover" DisplayMemberBinding="{Binding Path=coverOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Trans" DisplayMemberBinding="{Binding Path=transferOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Atraca" DisplayMemberBinding="{Binding Path=atracadoraOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Plancha" DisplayMemberBinding="{Binding Path=planchaOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Bonding" DisplayMemberBinding="{Binding Path=bondingOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="ZigZag" DisplayMemberBinding="{Binding Path=zigzagOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Multi" DisplayMemberBinding="{Binding Path=multiagujaOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Manual" DisplayMemberBinding="{Binding Path=manualOperario, StringFormat=P0}"/>
                                <GridViewColumn Header="Varias" DisplayMemberBinding="{Binding Path=variasOperario, StringFormat=P0}"/>
                            </GridView>
                        </ListView.View>
                    </ListView>
                </TabItem>
                <!--#endregion-->
                <!--#region rebalance-->
                <TabItem Header="Rebalance" x:Name="pest_rebalance" >
                    <Grid>
                        <!--#region filas/columnas-->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <!--#endregion-->
                        <!--#region botones-->
                        <Button x:Name="rebalance_bt" Grid.Column="0" Grid.Row="0" Background="Transparent" BorderBrush="Transparent" Grid.ColumnSpan="2" Click="rebalance_bt_Click">
                            <Image Source="/imagenes/verificacion.png"/>
                        </Button>
                        <Label Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" FontFamily="calibri" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top">Generar</Label>
                        <Button x:Name="actuali_balance" Grid.Column="2" Grid.Row="0" Background="Transparent" BorderBrush="Transparent"  Grid.ColumnSpan="2" Click="actuali_balance_Click">
                            <Image Source="/imagenes/expediente.png"/>
                        </Button>
                        <Label Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" FontFamily="calibri" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top">Enviar</Label>
                        <!--#endregion-->
                        <!--#region list_view-->
                        <ListView x:Name="rebalance_" Grid.Row="2" Grid.RowSpan="15"  Grid.Column="0" Grid.ColumnSpan="15" Margin="0,-35,0,0" FontSize="9" >
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="Operario" DisplayMemberBinding="{Binding Path=nombreOperario}" />
                                    <GridViewColumn Header="Operacion" DisplayMemberBinding="{Binding Path=tituloOperacion}"/>
                                    <GridViewColumn Header="SAM" DisplayMemberBinding="{Binding Path=samOperacion}"/>
                                    <GridViewColumn Header="Asignado" DisplayMemberBinding="{Binding Path=asignadoOperacion}"/>
                                    <GridViewColumn Header="Tiempo">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBox x:Name="tiempoTomado" Width="40" Text="{Binding Path=tiempoRebalance}" LostFocus="tiempo_tomado_LostFocus" KeyDown="soloNumerosDecimales"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Eficiencia" DisplayMemberBinding="{Binding Path=eficienciaRebalance, StringFormat=P}"/>
                                    <GridViewColumn Header="Carga" DisplayMemberBinding="{Binding Path=cargaRebalance}"/>
                                </GridView>
                            </ListView.View>
                        </ListView>
                        <!--#endregion-->
                    </Grid>
                </TabItem>
                <!--#endregion-->
            </TabControl>
            <!--#endregion-->
            <!--#region control_tab_maquinas-->
            <TabControl  Grid.Row="0" Grid.Column="14" Grid.RowSpan="48" Grid.ColumnSpan="26" Margin="5,0,5,5"  x:Name="control_tab_maquinas">
                <TabItem Header="Lay Out" TabIndex="0">
                    <ScrollViewer x:Name="espacio"  VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Visible" Grid.Row="0" Grid.Column="14" Grid.RowSpan="48" Grid.ColumnSpan="26" Height="auto" Width="auto">
                        <Viewbox  x:Name="ZoomViewbox" Stretch="Fill" Grid.Row="0" Grid.Column="14" Grid.RowSpan="48" Grid.ColumnSpan="26">
                            <Border BorderBrush="White" BorderThickness="2" Margin="10" >
                                <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="14" Grid.RowSpan="49" Grid.ColumnSpan="26" >
                                    <!--#region areaPreparacion-->
                                    <StackPanel Grid.Row="0" Grid.Column="14" Grid.RowSpan="49" Grid.ColumnSpan="4" x:Name="areaPreparacion">
                                        <!--#region estacion1-->
                                        <Border BorderBrush="Black" BorderThickness="1" Width="180" Padding="0" Margin="4" Background="White">
