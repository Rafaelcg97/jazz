<Page x:Class="Production_control_1._0.balance"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Production_control_1._0"
      xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
     xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      Title="balance" Background="white">
    <Grid Margin="5,0,0,5" x:Name="print">
        <!--#region filas/columnas-->
        <Grid.RowDefinitions>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="22*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <!--#endregion-->
        <!--#region control_general-->
        <StackPanel Orientation="Horizontal" Grid.ColumnSpan="12">
            <Button x:Name="salir" Width="20"  Click="salir_Click" Height="15" Margin="2,0">
                <Image Source="/imagenes/cancelar.png" />
            </Button>
            <Button x:Name="guardar" Width="20"  Click="guardar_Click" Height="15" Margin="2,0">
                <Image Source="/imagenes/disquete.png" />
            </Button>
            <Button x:Name="imprimir" Width="20"  Click="imprimir_Click" Height="15" Margin="2,0">
                <Image Source="/imagenes/impresora.png" />
            </Button>
            <Popup x:Name="confirmar_guard" Width="622" Height="200" Grid.ColumnSpan="14" StaysOpen="False">
                <Border Background="#F9FAFA" CornerRadius="10">
                    <StackPanel Orientation="Horizontal">
                    <StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                            <Label FontWeight="Bold" Background="#F9FAFA" Width="150" Margin="0,5,0,0">Modulo:</Label>
                            <TextBox x:Name="modulo_guard" Width="200" Margin="10,5,0,0" IsEnabled="False">-----</TextBox>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,5,0,0">
                            <Label FontWeight="Bold" Background="#F9FAFA" Width="150">Estilo:</Label>
                            <TextBox x:Name="estilo_guard" Width="200" Margin="10,0,0,0" IsEnabled="False"></TextBox>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,5,0,0">
                            <Label FontWeight="Bold" Background="#F9FAFA" Width="150" Margin="0">Temporada:</Label>
                            <TextBox x:Name="temporada_guard" Width="200" Margin="10,0,0,0" IsEnabled="False"></TextBox>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                            <Label FontWeight="Bold" Background="#F9FAFA" Width="150" Margin="0">Version:</Label>
                            <TextBox x:Name="version_guard" Width="200" Margin="10,0,0,0" KeyDown="solo_numeros"></TextBox>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,5,0,0" HorizontalAlignment="Center">
                            <Button x:Name="guard" Width="30" Height="30" Margin="10,0,10,0" Background="Transparent" BorderBrush="Transparent" Click="guard_Click">
                                <Image Source="/imagenes/disquete.png"></Image>
                            </Button>
                        </StackPanel>
                    </StackPanel>
                        <StackPanel Margin="38,0,0,0">
                            <Border BorderBrush="Black" BorderThickness="2" CornerRadius="10" Height="190">
                                <StackPanel>
                            <Label FontWeight="Bold">Versiones Existentes del estilo</Label>
                                <ListView x:Name="ultimos_estilos"  Height="150" Margin="5,0" Width="200">
                                        <ListView.View>
                                            <GridView>
                                                <GridViewColumn Header="Modulo" DisplayMemberBinding="{Binding Path=modulo}" Width="100"/>
                                                <GridViewColumn Header="Ver" DisplayMemberBinding="{Binding Path=version}" Width="25"/>
                                                <GridViewColumn Header="Fecha" DisplayMemberBinding="{Binding Path=fecha, StringFormat=0:d}" Width="75" />
                                            </GridView>
                                        </ListView.View>
                                    </ListView>
                                </StackPanel>
                            </Border>
                        </StackPanel>
                    </StackPanel>
                </Border>
            </Popup>
        </StackPanel>
        <!--#endregion-->

        <Border BorderBrush="Black" BorderThickness="1" Margin="2" Padding="2" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="6" Grid.RowSpan="7">
            <StackPanel Orientation="Horizontal">
                <StackPanel Margin="5">
                    <Label Content="Fecha"  Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="Estilo:"  Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="Temporada:"  Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="SAM:"  Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="Modulo:" Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="Versión:" Grid.Column="1"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2" Margin="0,2,0,0"/>
                </StackPanel>
                <StackPanel Margin="5">
                    <Label x:Name="fecha_" Content="----"   Grid.Column="3"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                    <Label x:Name="estilo_" Content="----"   Grid.Column="3"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                    <Label x:Name="temporada_" Content="----"  Grid.Column="3"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                    <Label x:Name="sam_" Content="----"   Grid.Column="3"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                    <StackPanel Orientation="Horizontal">
                        <ComboBox x:Name="modulo"  Grid.Row="5" Grid.Column="3" FontSize="14" FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2" Width="100" SelectionChanged="modulo_SelectionChanged"/>
                        <Button Grid.Column="4" Grid.ColumnSpan="1" Click="Button_Click_1">
                            <Image Source="imagenes/desconocido.png" Grid.ColumnSpan="2" Margin="0,0,0,0" Grid.RowSpan="1" Height="15" Width="31" UseLayoutRounding="False" RenderTransformOrigin="0.758,0.5"/>
                        </Button>
                        <Button x:Name="agregar_operario" Grid.Column="4" Grid.ColumnSpan="1" Click="agregar_operario_Click">
                            <Image Source="imagenes/nuevo_use.png" Grid.ColumnSpan="2" Margin="0,0,0,0" Grid.RowSpan="1" Height="15" Width="31" UseLayoutRounding="False" RenderTransformOrigin="0.758,0.5"/>
                        </Button>
                        <Popup x:Name="datos_operario" Width="420" Height="150">
                            <Border Background="#F9FAFA" CornerRadius="10">
                            <StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                                        <Label FontWeight="Bold" Background="#F9FAFA" Width="150" Margin="0,5,0,25">Código de Operario:</Label>
                                <TextBox x:Name="codigo_op" Width="200" Margin="10,5,0,25" TextChanged="codigo_op_TextChanged" ></TextBox>
                                        <Button x:Name="revision" Width="30" Height="30" Margin="0,5,0,25" Background="Transparent" BorderBrush="Transparent" IsEnabled="False" Visibility="Hidden">
                                            <Image Source="/imagenes/marca.png"></Image>
                                        </Button>
                                    </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                        <Label FontWeight="Bold" Background="#F9FAFA" Width="150">Nombre de Operario:</Label>
                                    <TextBox x:Name="nombre_op" Width="200" Margin="10,0,0,0"></TextBox>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,15,0,0" HorizontalAlignment="Center">
                                        <Button x:Name="agregar_op"  Width="30" Height="30" Margin="0,0,10,0" Background="Transparent" BorderBrush="Transparent" Click="agregar_op_Click">
                                            <Image Source="/imagenes/agregar-usuario.png"></Image></Button>
                                        <Button x:Name="no_nomb" Width="30" Height="30" Margin="10,0,10,0" Background="Transparent" BorderBrush="Transparent" Click="no_nomb_Click">
                                            <Image Source="/imagenes/desconocido.png"></Image>
                                        </Button>
                                        <Button x:Name="agregar_cancel" Width="30" Height="30" Margin="10,0,0,0" Click="agregar_cancel_Click" Background="Transparent" BorderBrush="Transparent">
                                            <Image Source="/imagenes/boton-x.png"></Image>
                                        </Button>
                                    </StackPanel>
                            </StackPanel>
                            </Border>
                        </Popup>
                    </StackPanel>
                    <TextBox x:Name="version_" Grid.Column="3" FontFamily="calibri" FontWeight="Medium" Grid.ColumnSpan="2" Margin="0,2,0,0" KeyDown="solo_numeros" ></TextBox>
                </StackPanel>
            </StackPanel>
        </Border>

        <Border BorderBrush="Black" BorderThickness="1" Margin="2" Padding="2" Grid.Column="6" Grid.Row="1" Grid.ColumnSpan="6" Grid.RowSpan="7">
            <StackPanel Orientation="Horizontal" Grid.ColumnSpan="3">
                <StackPanel Margin="5">
                    <Label Content="Piezas de la corrida:" Grid.Row="1" Grid.Column="8" FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="Horas de Corrida:"  Grid.Row="2" Grid.Column="8"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="Operarios:" Grid.Row="4" Grid.Column="8"  FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="Eficiencia:" Grid.Row="3" Grid.Column="8" FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                    <Label Content="Piezas por Hora:" Grid.Row="5" Grid.Column="8" FontFamily="calibri" FontWeight="Bold" Padding="0" Grid.ColumnSpan="2"/>
                </StackPanel>
                <StackPanel Margin="5" Grid.ColumnSpan="3">
                    <TextBox x:Name="piezas_de_corrida"  Grid.Row="2" Grid.Column="11"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2" MinWidth="50" MaxWidth="150" TextChanged="piezas_de_corrida_TextChanged" KeyDown="solo_numeros"/>
                    <TextBox x:Name="horas_de_corrida"  Grid.Row="1" Grid.Column="11"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"  MinWidth="50" MaxWidth="150" TextChanged="horas_de_corrida_TextChanged" KeyDown="solo_numeros"/>
                    <Label x:Name="operarios_" Content="0" Grid.Row="4" Grid.Column="11" FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2" MinWidth="50" MaxWidth="150" />
                    <Label x:Name="eficiencia_" Content="0"  Grid.Row="3" Grid.Column="11" FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"  MinWidth="50" MaxWidth="150"/>
                    <Label x:Name="piezas_por_hora" Content="0"  Grid.Row="5" Grid.Column="11"  FontFamily="calibri" FontWeight="Medium" Padding="0" Grid.ColumnSpan="2"/>
                </StackPanel>
            </StackPanel>
        </Border>

        <Border BorderBrush="Black" BorderThickness="1" Margin="2" Grid.Row="1" Grid.Column="12" Grid.RowSpan="7">
            <Slider x:Name="tamano"  Grid.Row="1" Grid.Column="12" Minimum="0" Maximum="2500" Orientation="Vertical" Grid.RowSpan="7" HorizontalAlignment="Center" ValueChanged="Slider_ValueChanged"/>
        </Border>
        <!--#region control_tab_de_datos-->
        <TabControl  Grid.Row="8" Grid.RowSpan="40" Grid.Column="0" Grid.ColumnSpan="13">
            <!--#region lista_de_operaciones-->
            <TabItem Header="Operaciones" ScrollViewer.HorizontalScrollBarVisibility="Visible">
                <ListBox x:Name="Operaciones" HorizontalAlignment="Stretch" FontSize="10" SelectionMode="Extended" Grid.Row="1" Grid.RowSpan="15"  Grid.Column="1" Grid.ColumnSpan="15" PreviewMouseLeftButtonDown="Operaciones_PreviewMouseLeftButtonDown">
                    <ListBox.ItemTemplate>
                        <DataTemplate>
                            <Grid Margin="0,2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="275"/>
                                    <ColumnDefinition Width="40"/>
                                    <ColumnDefinition Width="130"/>
                                    <ColumnDefinition Width="28"/>
                                    <ColumnDefinition Width="100"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="{Binding Title}"/>
                                <TextBlock Grid.Column="1" Text="{Binding sam_cod}"/>
                                <TextBlock Grid.Column="2" FontSize="8" Text="{Binding ajuste_cod}"/>
                                <TextBlock Grid.Column="3" Text="{Binding cap_cod}"/>
                                <Grid Grid.Column="4">
                                    <ProgressBar  Minimum="0" Maximum="1" Value="{Binding Completion}"/>
                                    <TextBlock Text="{Binding Completion, StringFormat=P1}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </Grid>
                            </Grid>
                        </DataTemplate>
                    </ListBox.ItemTemplate>
                </ListBox>
            </TabItem>
            <!--#endregion-->
            <!--#region lista_de_operarios-->
            <TabItem Header="Operarios">
                <StackPanel Grid.RowSpan="15">
                    <Grid Margin="0.2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="25"/>
                            <ColumnDefinition Width="90"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Text="NOMBRE" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="1"  Text="PL" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="2"  Text="RN" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="3"  Text="FS" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="4"  Text="CV" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="5"  Text="TF" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="6"  Text="AT" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="7"  Text="PC" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="8"  Text="BD" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="9"  Text="Z-Z" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="10"  Text="MA" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="11"  Text="ML" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="12"  Text="VR" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                        <TextBlock Grid.Column="13"  Text="ASIGNADO" FontSize="10" HorizontalAlignment="Center" FontWeight="Bold"/>
                    </Grid>
                    <ListBox x:Name="Operarios" HorizontalAlignment="Stretch"  SelectionMode="Extended" Grid.Row="1"  Grid.Column="1" Grid.ColumnSpan="11" PreviewMouseLeftButtonDown="Operarios_PreviewMouseLeftButtonDown" ScrollViewer.VerticalScrollBarVisibility="Visible" ScrollViewer.HorizontalScrollBarVisibility="Visible" MaxHeight="550" KeyDown="Operarios_KeyDown">
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <Grid Margin="0.2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="200"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="25"/>
                                        <ColumnDefinition Width="90"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0" Text="{Binding Title}" FontSize="10"/>
                                    <TextBlock Grid.Column="1"  Text="{Binding plana}" FontSize="10"/>
                                    <TextBlock Grid.Column="2"  Text="{Binding rana}" FontSize="10"/>
                                    <TextBlock Grid.Column="3"  Text="{Binding flat}" FontSize="10"/>
                                    <TextBlock Grid.Column="4"  Text="{Binding cover}" FontSize="10"/>
                                    <TextBlock Grid.Column="5"  Text="{Binding transfer}" FontSize="10"/>
                                    <TextBlock Grid.Column="6"  Text="{Binding atracadora}" FontSize="10"/>
                                    <TextBlock Grid.Column="7"  Text="{Binding plancha}" FontSize="10"/>
                                    <TextBlock Grid.Column="8"  Text="{Binding bonding}" FontSize="10"/>
                                    <TextBlock Grid.Column="9"  Text="{Binding zig_zag}" FontSize="10"/>
                                    <TextBlock Grid.Column="10"  Text="{Binding multiaguja}" FontSize="10"/>
                                    <TextBlock Grid.Column="11"  Text="{Binding manual}" FontSize="10"/>
                                    <TextBlock Grid.Column="12"  Text="{Binding varias}" FontSize="10"/>
                                    <Grid Grid.Column="13">
                                        <ProgressBar  Minimum="0" Maximum="1" Value="{Binding Completion}"/>
                                        <TextBlock Text="{Binding Completion, StringFormat=P1}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                    </Grid>
                                </Grid>
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                </StackPanel>
            </TabItem>
            <!--#endregion-->
            <!--#region rebalance-->
            <TabItem Header="Rebalance" x:Name="pest_rebalance" >
                <Grid>
                    <!--#region filas/columnas-->
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <!--#endregion-->
                    <!--#region botones-->
                    <Button x:Name="rebalance_bt" Grid.Column="0" Grid.Row="0" Background="Transparent" BorderBrush="Transparent" Grid.ColumnSpan="2" Click="rebalance_bt_Click">
                        <Image Source="/imagenes/verificacion.png"/>
                    </Button>
                    <Label Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" FontFamily="calibri" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top">Generar</Label>
                    <Button x:Name="actuali_balance" Grid.Column="2" Grid.Row="0" Background="Transparent" BorderBrush="Transparent"  Grid.ColumnSpan="2" Click="actuali_balance_Click">
                        <Image Source="/imagenes/expediente.png"/>
                    </Button>
                    <Label Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" FontFamily="calibri" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top">Enviar</Label>
                    <!--#endregion-->
                    <!--#region list_view-->
                    <ListView x:Name="rebalance_" Grid.Row="2" Grid.RowSpan="15"  Grid.Column="0" Grid.ColumnSpan="15" Margin="0,-35,0,0" >
                    <ListView.View>
                        <GridView>
                            <GridViewColumn Header="Operario" DisplayMemberBinding="{Binding Path=operario}" />
                            <GridViewColumn Header="Operacion" DisplayMemberBinding="{Binding Path=operacion}"/>
                            <GridViewColumn Header="SAM" DisplayMemberBinding="{Binding Path=sam}"/>
                            <GridViewColumn Header="Asignado" DisplayMemberBinding="{Binding Path=asignado}"/>
                            <GridViewColumn Header="Tiempo">
                                <GridViewColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBox x:Name="tiempo_tomado" Width="40" Text="{Binding Path=tiempo}" LostFocus="tiempo_tomado_LostFocus"/>
                                    </DataTemplate>
                                </GridViewColumn.CellTemplate>
                            </GridViewColumn>
                            <GridViewColumn Header="Eficiencia" DisplayMemberBinding="{Binding Path=eficiencia, StringFormat=P}"/>
                            <GridViewColumn Header="Carga" DisplayMemberBinding="{Binding Path=carga}"/>
                        </GridView>
                    </ListView.View>
                </ListView>
                    <!--#endregion-->
                </Grid>
            </TabItem>
            <!--#endregion-->
        </TabControl>
        <!--#endregion-->
        <!--#region control_tab_maquinas-->
        <TabControl  Grid.Row="0" Grid.Column="14" Grid.RowSpan="48" Grid.ColumnSpan="26" Margin="5,0,5,5">
            <TabItem Header="Lay Out">
                <ScrollViewer x:Name="espacio"  VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Visible" Grid.Row="0" Grid.Column="14" Grid.RowSpan="48" Grid.ColumnSpan="26" Height="auto" Width="auto">
                    <Viewbox  x:Name="ZoomViewbox" Stretch="Fill" Grid.Row="0" Grid.Column="14" Grid.RowSpan="48" Grid.ColumnSpan="26">
                        <Border BorderBrush="White" BorderThickness="2" Margin="10" >
                            <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="14" Grid.RowSpan="49" Grid.ColumnSpan="26" >

                                <StackPanel Grid.Row="0" Grid.Column="14" Grid.RowSpan="49" Grid.ColumnSpan="4">

                                    <Border x:Name="b1" Grid.Row="1" Grid.Column="14" Grid.RowSpan="4" Grid.ColumnSpan="4" BorderBrush="Black" BorderThickness="1" Margin="4" Background="White">
                                        <StackPanel>
                                            <Grid>
                                                <TextBox Background="Transparent" BorderBrush="Transparent" Foreground="Black" x:Name="maquina1"  FontSize="10" MaxHeight="15" MinHeight="15" FontWeight="Bold" MinWidth="180" MaxWidth="180" ></TextBox>
                                                <TextBox  x:Name="categoria1" Visibility="Hidden"/>
                                            </Grid>
                                            <ProgressBar x:Name="asignado1" MaxHeight="5" MinHeight="5" Minimum="0" Maximum="1" Value="0" Foreground="#00FF00"/>
                                            <StackPanel Orientation="Horizontal">
                                                <RadioButton x:Name="externo_externo" Background="Green" Checked="externo_externo_Checked" />
                                                <RadioButton x:Name="externo_interno" Background="Orange" Checked="externo_interno_Checked" />
                                                <RadioButton x:Name="interno_externo" Background="Yellow" Checked="interno_externo_Checked" />
